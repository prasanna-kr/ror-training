<!DOCTYPE html>
<html>
  <head>
    <title>Mvc</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>

    <!-- Favicons -->
    <link href="/assets/favicon.png" rel="icon">
    <link href="/assets/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <%# <link href="pulic/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="pulic/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="pulic/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="pulic/vendor/quill/quill.snow.css" rel="stylesheet">
    <link href="pulic/vendor/quill/quill.bubble.css" rel="stylesheet">
    <link href="pulic/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="pulic/vendor/simple-datatables/style.css" rel="stylesheet"> %>

    <!-- Template Main CSS File -->
    <link href="/assets/style.css" rel="stylesheet">
  </head>

  <body>
    <!-- ======= Header ======= -->
    <%= render 'layouts/header' %>
    <!-- ======= Sidebar ======= -->
    <%= render 'layouts/sidebar'%>
    <main id="main" class="main">
      <%= yield %>
    </main>
  </body>
</html>
